{
    "$schema": "./schema.json",
    "type": "external",
    "name": "tshark",
    "description": "pcapファイルを解析する",
    "execution": {
        "command": [
            "tshark",
            "{input.0}"
        ]
    },
    "prepare-module-directory": false,
    "data": {
        "input": {
            "type": "json"
        },
        "output": {
            "type": "json"
        }
    }
}