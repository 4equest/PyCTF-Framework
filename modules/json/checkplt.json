{
    "$schema": "./schema.json",
    "module": {
        "type": "external",
        "name": "checkplt"
    },
    "execution": {
        "environment": {
            "type": "venv"
        },
        "command": [
            "python3",
            "{cwd}/modules/checkplt/checkplt.py",
            "{cwd}/{input.0}",
            "--json"
        ]
    },
    "prepare-module-directory": true,
    "data": {
        "input": {
            "type": "json"
        },
        "output": {
            "type": "json"
        }
    }
}