{
    "$schema": "./schema.json",
    "type": "external",
    "name": "xxd",
    "description": "Hex dump a file or standard input.",
    "execution": {
        "command": [
            "xxd",
            "{input.0}"
        ]
    },
    "prepare-module-directory": false,
    "data": {
        "input": {
            "type": "json"
        },
        "output": {
            "type": "json"
        }
    }
}