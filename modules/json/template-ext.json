{
    "$schema": "./schema.json",
    "module": {
        "type": "external",
        "name": "fuga"
    },
    "execution": {
        "command": [
            "python",
            "fuga.py",
            "{input.0}",
            "{module_dir}"
        ],
        "environment": {
            "type": "venv"
        }
    },
    "prepare-module-directory": true,
    "data": {
        "input": {
            "type": "file",
            "path": "input_temp.json"
        },
        "output": {
            "type": "file",
            "path": "output_temp.json"
        }
    }
}